{
    "name": "eboom",
    "description": "Boom responses implementation for ExpressJS.",
    "version": "0.0.2",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "/lib/**/*"
    ],
    "author": "Vladimír Fojtík <fojtik.v@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watchAll"
    },
    "keywords": [
        "Boom",
        "Express",
        "ExpressJS",
        "responses"
    ],
    "dependencies": {
        "boom": "^7.3.0",
        "express": "^4.16.4"
    },
    "devDependencies": {
        "@types/express": "^4.16.1",
        "@types/jest": "^24.0.9",
        "jest": "^24.1.0",
        "supertest": "^3.4.2",
        "ts-jest": "^24.0.0",
        "tslint": "^5.13.1",
        "tslint-config-airbnb": "^5.11.1",
        "typescript": "^3.3.3333"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/Fir3st/eboom.git"
    },
    "bugs": {
        "url": "https://github.com/Fir3st/eboom/issues"
    },
    "homepage": "https://github.com/Fir3st/eboom",
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/lib/"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
